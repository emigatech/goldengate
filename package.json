{
  "name": "goldengate",
  "version": "0.2.0",
  "description": "GoldenGate is dynamic/static web scraper, and It is integrated to Google Cloud Storage and Firebase Realtime DB",
  "main": "service/server.js",
  "scripts": {
    "dev": "nodemon service/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emigatech/goldengate.git"
  },
  "author": "Emin Muhammadi <muemin17631@sabah.edu.az> (https://github.com/eminmuhammadi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emigatech/goldengate/issues"
  },
  "homepage": "https://github.com/emigatech/goldengate#readme",
  "dependencies": {
    "@google-cloud/storage": "5.1.2",
    "api-error-handler": "1.0.0",
    "apicache": "1.5.3",
    "bluebird": "3.7.2",
    "compression": "1.7.4",
    "connect-timeout": "1.9.0",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "email-validator": "2.0.4",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "express-slow-down": "1.3.1",
    "firebase-admin": "9.0.0",
    "fs": "0.0.1-security",
    "helmet": "3.23.3",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "node-fetch": "2.6.0",
    "path": "0.12.7",
    "puppeteer": "5.0.0",
    "qs-stringify": "1.2.1",
    "scraperjs": "1.2.0",
    "uuid": "8.2.0",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "nodemon": "2.0.4"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
